{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:next": "next dev",
    "dev:server": "node server/server.ts",
    "dev": "NEXT_PUBLIC_NEON_URL=$(cat .env.local | grep NEXT_PUBLIC_NEON_URL | cut -d '=' -f2) concurrently \"npm:dev:next\" \"npm:dev:server\"",
    "prod": "NODE_ENV=production NEXT_PUBLIC_NEON_URL=$(cat .env.local | grep NEXT_PUBLIC_NEON_URL | cut -d '=' -f2) concurrently \"npm:dev:next\" \"npm:dev:server\"",
    "dev-https": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "kill:3001": "lsof -ti :3001 | xargs kill -9 || true"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.7",
    "http-proxy": "^1.18.1",
    "lucide-react": "^0.469.0",
    "next": "^15.0.0",
    "next-themes": "^0.4.4",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^4.12.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.17",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.0",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
